<div class="container">
	<h1 class="text-center">Pokemon Specifications / Data</h1>
		<div class="well">
			<h2 class="text-center">Name</h2>
			<h3 class="text-center"><%= pokemonData.name %></h3>
		</div>
		<div class="well">
			<h2 class="text-center">Height</h2>
			<h3 class="text-center"><%= pokemonData.height%> feet</h3>
		</div>
		<div class="well">
			<h2 class="text-center">Weight</h2>
			<h3 class="text-center"><%= pokemonData.weight%></h3>
		</div>
		<div class="well">
			<h2 class="text-center">Abilities</h2>
			<% pokemonData.abilities.forEach(ability => { %>
			<div class="abilities">
				<h3 class="text-center"><%= ability.ability.name %></h3>
			</div>

			<% }) %>
		<div>
</div>	


